<script>
    function showAlert() {
        alert("xin chao")
    }
    function showConfirm() {
        confirm("ok  nhe")
    }
    function showConsole() {
        console.log("hai dang")
    }
    function showDocumentWite() {
        document.write("alo khoe khong")

    }
    function showDocu() {
        document.getElementById("showCu").innerHTML=" <h1>ok the nhe</h1> "


    }

